	"item": [
		{
			"name": "e2e_auth",
			"item": [
				{
					"name": "register",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseJSON = pm.response.json();",
									"",
									"// check that response status HTTP 201 CREATED",
									"pm.test(\"Registration successful\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"// Save env vars",
									"pm.environment.set(\"username\", responseJSON.username);",
									"",
									"// check that 'username' from environment & response are equal",
									"pm.test(\"Response contains 'username' property\", () => {",
									"  pm.expect(pm.environment.get('username')).to.eql(responseJSON.username);",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"user3\",\n    \"password\": \"validpassword\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{LOCAL_URL}}/register",
							"host": [
								"{{LOCAL_URL}}"
							],
							"path": [
								"register"
							]
						}
					},
					"response": []
				},
				{
					"name": "login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseJSON = pm.response.json();",
									"",
									"// check that response status HTTP 200",
									"pm.test(\"Login successful\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"// Save env vars",
									"pm.environment.set(\"access_token\", responseJSON.accessToken);",
									"pm.environment.set(\"refresh_token\", responseJSON.refreshToken);",
									"",
									"// check that 'access_token' from environment & response are equal",
									"pm.test(\"Response contains 'access_token' property\", () => {",
									"  pm.expect(pm.environment.get('access_token')).to.eql(responseJSON.accessToken);",
									"});",
									"",
									"// check that 'refresh_token' from environment & response are equal",
									"pm.test(\"Response contains 'refresh_token' property\", () => {",
									"  pm.expect(pm.environment.get('refresh_token')).to.eql(responseJSON.refreshToken);",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"{{username}}\",\n    \"password\": \"{{password}}\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{LOCAL_URL}}/login",
							"host": [
								"{{LOCAL_URL}}"
							],
							"path": [
								"login"
							]
						}
					},
					"response": []
				},
